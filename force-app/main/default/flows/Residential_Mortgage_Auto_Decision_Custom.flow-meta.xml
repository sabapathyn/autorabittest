    <decisions>
        <name>Decision</name>
        <label>Decision</label>
        <locationX>831</locationX>
        <locationY>1599</locationY>
        <defaultConnector>
            <targetReference>Result_Recommended_for_Declination</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Recommended for Decline</defaultConnectorLabel>
        <rules>
            <name>Recommend_for_Approval</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AssignedPoints</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>4.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Recommended_for_Approval</targetReference>
            </connector>
            <label>Recommend for Approval</label>
        </rules>
        <rules>
            <name>Manual_Review_Required</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AssignedPoints</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>2.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>AssignedPoints</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>3.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Result_Manual_Review_Required</targetReference>
            </connector>
            <label>Manual Review Required</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determine if credit score is high enough to be approved.</description>
        <name>Evaluate_Credit_Score</name>
        <label>Evaluate Credit Score</label>
        <locationX>810</locationX>
        <locationY>520</locationY>
        <defaultConnector>
            <targetReference>Credit_Score_Add_2_Points_0</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Credit Score &gt; 750</defaultConnectorLabel>
        <rules>
            <name>Credit_Score_600</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>BestBorrowerCreditScore</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>600.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Credit_Score_Add_0_Points</targetReference>
            </connector>
            <label>Credit Score &lt; 600</label>
        </rules>
        <rules>
            <name>Credit_Score_Between_600_and_750</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>BestBorrowerCreditScore</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>600.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>BestBorrowerCreditScore</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>750.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Credit_Score_Add_1_Point_0</targetReference>
            </connector>
            <label>Credit Score Between 600 and 750</label>
        </rules>
    </decisions>
    <decisions>
        <description>Evaluate DTI Ratio</description>
        <name>Evaluate_DTI_Ratio</name>
        <label>Evaluate DTI Ratio</label>
        <locationX>820</locationX>
        <locationY>920</locationY>
        <defaultConnector>
            <targetReference>DTI_Add_2_Points_0_0</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>DTI &lt; 15</defaultConnectorLabel>
        <rules>
            <name>DTI_35</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Loan.LLC_BI__Debt_To_Income_Ratio__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>35.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>DTI_Add_0_Points</targetReference>
            </connector>
            <label>DTI &gt; 35</label>
        </rules>
        <rules>
            <name>DTI_Between_15_and_35</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Loan.LLC_BI__Debt_To_Income_Ratio__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>15.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loan.LLC_BI__Debt_To_Income_Ratio__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>35.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>DTI_Add_1_Point_0</targetReference>
            </connector>
            <label>DTI Between 15% and 35%</label>
        </rules>
    </decisions>
    <decisions>
        <description>Determines which borrower has the highest credit score.</description>
        <name>Is_Best_Borrower</name>
