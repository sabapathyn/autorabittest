<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LLC_BI__Is_Individual__c</fullName>
    <deprecated>false</deprecated>
    <description>This field is a formula field that assists in specifying if the account is an individual account.</description>
    <externalId>false</externalId>
    <formula>IF(OR(ISPICKVAL(Type, &apos;Sole Proprietorship&apos;),ISPICKVAL(Type, &apos;Individual&apos;)), 1, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Individual</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
