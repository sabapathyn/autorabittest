        <locationX>1089</locationX>
        <locationY>236</locationY>
        <defaultConnector>
            <targetReference>Get_Best_Credit_Score</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
